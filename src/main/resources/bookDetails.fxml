      <ChoiceBox fx:id="genre" layoutX="150.0" layoutY="300.0" prefHeight="25.0" prefWidth="250.0" />
      <Text fx:id="addingMessage" layoutX="441.0" layoutY="390.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Text fx:id="errorMessage" layoutX="492.0" layoutY="330.0" strokeType="OUTSIDE" strokeWidth="0.0" />
