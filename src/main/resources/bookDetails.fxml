      <Button fx:id="addingButton" layoutX="492.0" layoutY="342.0" mnemonicParsing="false" onAction="#handleAddingAction" prefHeight="32.0" prefWidth="95.0" style="-fx-background-color: #f0f8ff;" text="Add book" underline="true">
         <font>
            <Font name="Bell MT Bold" size="16.0" />
